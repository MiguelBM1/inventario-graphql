type Query {
    listarProductos: [Producto],
    buscarProductoById(id: String): Producto,
    listarCategorias: [Categoria],
    buscarCategoriaById(id: Float): Categoria
}

type Producto {
    id: String,
    nombre: String,
    precio: Float,
    cantidad: Int,
    categoria: Categoria
}

type Categoria {
    id: Float,
    nombre: String
    Productos: [Producto]
}

